<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="Entidades">
    <class name="Partidos" table="partidos">
        <id name="codigo" column="codigo">
            <generator class="native">
            </generator>
        </id>
        <property name="equipo_local" />
        <property name="equipo_visitante" />
        <property name="puntos_local" />
        <property name="puntos_visitante" />
        <property name="temporada" />
    </class>
</hibernate-mapping>